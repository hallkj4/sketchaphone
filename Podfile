target 'sketchaphone' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'FacebookCore'
  pod 'FacebookLogin'
  
  pod 'AWSAppSync'
  pod 'AWSS3'
#  pod 'AWSUserPoolsSignIn'
#  pod 'AWSAuthUI'
  pod 'AWSCognitoIdentityProvider'

  pod 'Kingfisher'
  pod 'SQLite.swift'
  pod 'Google-Mobile-Ads-SDK'
end

